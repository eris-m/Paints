<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Paints.ViewModels"
        x:Class="Paints.Dialogues.ChangeStockDialogue"
        x:DataType="vm:InfoPageViewModel"
        Width="300" Height="100"
        Title="Change stock level - Paints">
        <DockPanel>
                <Grid DockPanel.Dock="Bottom" ColumnDefinitions="*,*">
                        <Button Grid.Column="0" HorizontalAlignment="Stretch" IsCancel="True" Click="Cancel_OnClick">Cancel</Button>
                        <Button Grid.Column="1" HorizontalAlignment="Stretch" IsDefault="True" Click="Set_OnClick">Set</Button>
                </Grid>
                
                <MaskedTextBox VerticalAlignment="Top" Mask="000" Name="TextBox"/>
        </DockPanel>

</Window>
